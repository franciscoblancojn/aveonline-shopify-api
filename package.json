{
  "name": "aveonlineshopifynodejs",
  "version": "1.0.0",
  "description": "app of shopify for aveonline with nodejs",
  "main": "src/api.js",
  "scripts": {
    "start": "node src/api.js",
    "dev": "nodemon --watch src src/api.js",
    "test": "nodemon src/test.js",
    "pm2init": "pm2 start src/api.js && pm2 save",
    "pm2restart": "pm2 restart src/api.js && pm2 save",
    "pull": "git pull origin master && npm run pm2restart",
    "pulldev": "git pull origin master && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/franciscoblancojn/aveonline-shopify-nodejs.git"
  },
  "author": "blancofrancisco34@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/franciscoblancojn/aveonline-shopify-nodejs/issues"
  },
  "homepage": "https://gitlab.com/franciscoblancojn/aveonline-shopify-nodejs#readme",
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fmiddlewares": "*",
    "fs": "*",
    "module-alias": "^2.2.2",
    "mongodb": "^4.1.3",
    "monk": "^7.3.4",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.12",
    "nonce": "^1.0.4",
    "path": "^0.12.7",
    "querystring": "^0.2.1",
    "request-promise": "^4.2.6"
  },
  "_moduleAliases": {
    "@app": "src/",
    "@controllers": "src/controllers/",
    "@functions": "src/functions/",
    "@middlewares": "src/middlewares/",
    "@page": "src/page/",
    "@shopify": "src/shopify/",
    "@routes": "src/routes/"
  },
  "devDependencies": {
    "prisma": "^2.27.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
